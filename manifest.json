{
    "manifest_version": 2,
    "name": "WebPages",
    "description": "Your basic Hello World extension!",
    "version": "1.0",
    "author": "lixianfa",
    "applications": {
        "gecko": {
            "id": "helloworld@yoursite.com",
            "strict_min_version": "91.9.0"
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "messagesRead",
        "accountsRead"
    ],
    "icons": {
        "64": "images/internet.png",
        "32": "images/internet-32px.png",
        "16": "images/internet-16px.png"
    }
}